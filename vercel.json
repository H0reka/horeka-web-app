{
   "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/public/sitemap.xml"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/error", "dest": "/api/error.js" },
    {
      "src": "/",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/about",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/contact",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/faq",
      "dest": "/",
      "status": 200
    },
     {
      "src": "/sitemap.xml",
      "dest": "/public/sitemap.xml",
      "status": 200
    },
    {
      "src": "/products",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/terms",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/error"
    }
  ]
}
