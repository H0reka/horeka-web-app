{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self';"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer"
        }
      ]
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/error", "dest": "/api/error.js" },
    {
      "src": "/",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/about",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/contact",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/faq",
      "dest": "/",
      "status": 200
    },
     {
      "src": "/sitemap.xml",
      "dest": "/public/sitemap.xml",
      "status": 200
    },
    {
      "src": "/products",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/terms",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/error"
    }
  ]
}
